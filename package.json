{
  "name": "prevent-undefined",
  "version": "0.2.37",
  "description": "prevent-undefined makes objects to throw errors when any code refers an undefined propery on it.",
  "main": "index.mjs",
  "type": "module",
  "exports": {
    "./common.js": {
      "default": "./index.cjs"
    },
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "node:test-esm": "jest index.test.mjs",
    "node:test-cjs": "jest index.test.cjs",
    "test-esm": "NODE_OPTIONS='--experimental-vm-modules' node /usr/local/bin/jest --watch index.test.mjs",
    "test-cjs": "                                         node /usr/local/bin/jest --watch index.test.cjs",
    "test": "npm run make; npm run test-esm # ; npm run test-cjs",
    "make": "node ./make.js"
  },
  "files": [
    "README.md",
    "index.cjs",
    "index.mjs",
    "package.json"
  ],
  "author": {
    "name": "Atsushi Oka",
    "url": "https://github.com/a-oka-z/prevent-undefined"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/*.test.[cm]js"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-oka-z/prevent-undefined"
  },
  "license": "ISC",
  "devDependencies": {
    "sqlmacro": "^0.1.0"
  },
  "dependencies": {
    "vanilla-schema-validator": "^3.0.0"
  }
}

